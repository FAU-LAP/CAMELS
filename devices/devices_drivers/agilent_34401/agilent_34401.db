record(ai, "$(SETUP):agilent_34401:mesDCV")
{
    field (DESC, "Measure Voltage")
	field (DTYP, "stream")
    field (INP, "@agilent_34401.proto mesDCV $(PORT) $(G)")
    field (PINI, "0")
    field (TPRO, "1")
	field (SCAN, "Passive")
	field (MDEL, "-1")
}

record(ai, "$(SETUP):agilent_34401:mesDCV:trig")
{
	field (FLNK, "$(SETUP):agilent_34401:mesDCV")
}


